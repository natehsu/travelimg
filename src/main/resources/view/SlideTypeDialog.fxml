    <children>
        <HBox spacing="32">
            <padding>
                <Insets top="32" bottom="32" left="32" right="32"/>
            </padding>
            <children>
                <Button fx:id="mapButton" text="KARTENFOLIE" styleClass="btn"></Button>
                <Button fx:id="titleButton" text="TITELFOLIE" styleClass="btn"></Button>
            </children>
        </HBox>
    </children>
</fx:root>
