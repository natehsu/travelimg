<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>

<fx:root fx:id="root" prefHeight="Infinity" prefWidth="Infinity" styleClass="dialog" stylesheets="@style.css"
         type="javafx.scene.layout.BorderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
     <center>
        <ImageView fx:id="imageView"/>
     </center>
     <left>
        <Button fx:id="bt_previous" minWidth="62.0" minHeight="40.0" onAction="#bt_previousPressed" text="&lt;" BorderPane.alignment="CENTER_LEFT" />
     </left>
     <right>
        <Button fx:id="bt_next" minWidth="62.0" onAction="#bt_nextPressed" minHeight="40.0" text="&gt;" BorderPane.alignment="CENTER_RIGHT"/>
     </right>
</fx:root>
