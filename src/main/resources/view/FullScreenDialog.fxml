<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" fx:id="root" type="javafx.scene.layout.AnchorPane"
         xmlns="http://javafx.com/javafx/8">
    <children>
        <ImageView fx:id="imageView"/>
        <Button fx:id="bt_previous" minHeight="40.0" minWidth="62.0" onAction="#bt_previousPressed" text="&lt;"
                AnchorPane.bottomAnchor="400.0" AnchorPane.leftAnchor="0.0"/>
        <Button fx:id="bt_next" minHeight="40.0" minWidth="62.0" onAction="#bt_nextPressed" text="&gt;"
                AnchorPane.bottomAnchor="400.0" AnchorPane.rightAnchor="0.0"/>
        <HBox fx:id="raitingContainer" visible="true"></HBox>
    </children>

</fx:root>
